class concatination
{
	public static void main(String[] args) 
	{

		System.out.println(10+20); //16
		System.out.println('c'+10); //16
		System.out.println("100"+20); //16
		System.out.println("sheela"+10); //16
		System.out.println(10+"laila"+(400+20)); //16
		System.out.println(10+20+"hello"+40); //16
		System.out.println("ram"+'a'); //16
		System.out.println("kamla"+true); //16
		System.out.println("hello"+"world"); //16
	}
}
